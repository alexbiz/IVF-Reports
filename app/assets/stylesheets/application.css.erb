/*
 * This is a manifest file that'll automatically include all the stylesheets available in this directory
 * and any sub-directories. You're free to add application-wide styles to this file and they'll appear at
 * the top of the compiled file, but it's generally better to create a new file per style scope.
 *= require_self
 *= require_tree . 
*/

/*FONTS*/
body {font-family:"Quicksand", Arial, sans-serif;font-weight:400;color:#7a7a7a;}
h1, h2, h3, h4, h5 {font-family: "Lato", Arial, sans-serif;font-weight:400;}

/*BACKGROUND*/
.app_bg {
	background:#7e9ba6;
}

/*STAR COLORS*/
.green_star{color:#009900;}
.blue_star{color:#000099;}
.yellow_star{color:#FFB00F;}
.gray_star{color:#7a7a7a;}
/*Flash Messages*/
.flash{position:relative;display:inline-block;width:916px;;text-align:center;font-size:16px;height:40px;line-height:40px;margin-left:12px;margin-right:12px;margin-bottom:24px;}
.flash.error{color:#990000;}
.flash.success{color:#0a3445;}
.flash span.flash_close {display:inline-block;position:relative;right:12px;float:right;}
.flash span.flash_close:hover {font-weight:bold;cursor:pointer;font-size:18px;}
.flash span.flash_close span.icon {}
	
/*WRAPPER*/
.wrapper {
	background:#ffffff;display:inline;float:left;box-shadow: 0px 0px 6px 6px #6b8b96;border-bottom:1px solid #cccccc;
}

/*Subtitle*/
.subtitle{
	margin-left:12px;margin-top:-24px;font-size:12px;margin-bottom:24px;
}

/*Breadcrumbs*/
.breadcrumbs{
	height:20px;margin-bottom:12px;margin-left:12px;
}

/*HEADER AND NAVIGATION*/
div.header_top {display:block;background:#ffffff;height:100px;}
span.logo {background: url("<%= asset_path 'logo.png' %>") no-repeat center left;display:inline-block;height:100px;width:250px;float:left;}
/*span.logoicon {background: url("<%= asset_path 'favicon.png' %>") no-repeat center center;display:inline-block;width:80px;height:80px;float:left;}*/
span.logo a {display:block;height:100px;}
span.authentication {display:inline-block;width:600px;float:right;height:70px;text-align:right;margin-top:30px;}
span.authentication ul {display:inline-block;width:100%;margin:0px;padding:0px;float:right;}
span.authentication ul li {display:inline-block;width:20%;margin:0px;padding:0px;list-style-type:none;height:40px;margin-left:5px;text-align:center;line-height:40px;float:right;overflow:hidden;}
span.authentication ul li span.icon {line-height:50px;}
span.authentication ul li.divider {width:10px;}
span.authentication ul li a {text-decoration:none;color:#325d64;font-size:13px;}
span.authentication ul li a.auth {color:#ffffff;font-weight:bold;}


div.header_nav {display:block;font-family: 'Lato', Arial, sans-serif;font-weight:300;}
nav.header ul {display:inline-block;width:100%;margin:0px;padding:0px;float:left;height:40px;margin-bottom:24px;}
nav.header ul li {display:inline-block;width:15%;height:34px;line-height:34px;text-align:center;float:left;list-style-type:none;border-right:1px solid #5e818a;border-left:1px solid #3c6066;margin-top:4px;margin-bottom:2px;}
nav.header ul li.first {border-left:0px;}
nav.header ul li.last {border-right:0px;}
nav.header ul li.selected {}
nav.header ul li a {text-decoration:none;color:#ffffff;display:block;height:38px;}
nav.header ul li a:hover {color:#cccccc;}
nav.header ul li.search {color:#ffffff;width:20%;text-align:right;float:left;font-size:13px;border-right:0px;border-left:0px;}
nav.header ul li.search form {display:inline-block;}
nav.header ul li.search form input.search_box {display:inline-block;width:120px;padding-left:12px;}
nav.header ul li.search:hover {color:#cccccc;}

.nav_bg {
background: #6b8b96; /* Old browsers */
/* IE9 SVG, needs conditional override of 'filter' to 'none' */
background: url(data:image/svg+xml;base64,);
background: -moz-linear-gradient(top, #6b8b96 0%, #325e63 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#6b8b96), color-stop(100%,#325e63)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top, #6b8b96 0%,#325e63 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top, #6b8b96 0%,#325e63 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top, #6b8b96 0%,#325e63 100%); /* IE10+ */
background: linear-gradient(to bottom, #6b8b96 0%,#325e63 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#6b8b96', endColorstr='#325e63',GradientType=0 ); /* IE6-8 */
}

/*YIELD CONTENT*/
.yield_content {}
.yield_content h1 {font-family:'Lato',Arial,sans-serif;font-weight:400;color:#6b8b96;font-size:24px;margin-bottom:24px;margin-left:10px;}
.yield_content h1.centered {text-align:center;}
.yield_content .third {}
.yield_content .twothirds {}
.yield_content .twothirds.left {margin-left:10px;width:610px;}
.yield_content .twothirds.right {margin-right:10px;width:610px;}
.yield_content .third.left {margin-left:10px;width:290px;}
.yield_content .third.right {margin-right:10px;width:290px;}
.yield_content .third.middle {}
.yield_content .full_page {margin-left:10px;margin-right:10px;width:920px;}
.yield_content h2 {color:#6b8b96;font-family:'Lato', Arial, sans-serif;font-weight:400;font-size:16px;margin-bottom:12px;}
.yield_content h2.welcome {color:#0a3445; font-size:18px;font-weight:400;border-bottom:1px solid #0a3445;padding-bottom:6px;}
.yield_content p {text-align:justify;margin-bottom:15px;font-size:14px;line-height:20px;font-family:'Quicksand', Arial, sans-serif;font-weight:400;color:#7a7a7a;}
.yield_content hr {width:95%;margin-top:20px;margin-bottom:20px;border:0px solid #0F5151;background:#0F5151;height:1px;margin-left:auto;margin-right:auto;opacity:0.3;}


/*FOOTER*/
div.footer {background:#e5e5e5;width:100%;position:relative;}
div.footer h3 {color:#6b8b96;font-family:'Lato', Arial, sans-serif;font-weight:300;margin-bottom:12px;margin-top:12px;}
div.footer ul {display:inline-block;width:100%;padding:0px;margin:0px;float:left;}
div.footer ul li {list-style-type:none;float:left;margin:0px;padding:0px;display:inline-block;width:100%;color:#8b8b8b;font-size:13px;font-family:'Quicksand',Arial,sans-serif;font-weight:400;height:24px;}
div.footer ul li a {text-decoration:none;display:block;color:#8b8b8b;font-size:13px;font-family:'Quicksand',Arial,sans-serif;font-weight:400;}
div.footer ul li a:hover {color:#6b8b96;}
div.footer_text {text-align:center;color:#ffffff;font-size:12px;font-family:'Quicksand',Arial,sans-serif;padding-top:12px;padding-bottom:12px;background:#7795a1;}
div.footer_text a {text-decoration:none;color:#ffffff;font-weight:bold;}

/*ERRORS*/
div#error_explanation {border:1px solid #990000;font-family:'Quicksand',Arial,sans-serif;font-weight:400;}
div#error_explanation h2 {color:#990000;padding:6px;font-weight:bold;}
div#error_explanation ul {padding:0px;margin:0px;list-style-type:none;padding:6px;margin-left:12px;}
div#error_explanation ul li {padding:0px;margin:0px;list-style-type:square;color:#990000;font-size:14px;}


/*ADVERTISEMENT*/
div.advertisement{background:#7795a1;padding:12px;margin-bottom:24px;}
div.advertisement h2{color:#0b3542;font-size:18px;border-bottom:1px solid #0b3542;padding-bottom:6px;margin-left:6px;margin-right:6px;}
div.advertisement img {margin-top:48px;}
div.advertisement .learn_more {text-align:center;margin-top:48px;margin-bottom:12px;}

/*ROUND STUFF*/
/* round stuff */
.round {-moz-border-radius:10px;-webkit-border-radius:10px;border-radius:10px;}
.small-round {-moz-border-radius:4px;-webkit-border-radius:4px;border-radius:4px;}
.medium-round {-moz-border-radius:15px;-webkit-border-radius:15px;border-radius:15px;}
.rounder {-moz-border-radius:20px;-webkit-border-radius:20px;border-radius:20px;}
.bottom-round {-moz-border-radius-bottomright:10px;-moz-border-radius-bottomleft:10px;border-bottom-left-radius:10px;border-bottom-right-radius:10px;-webkit-border-bottom-left-radius:10px;-webkit-border-bottom-right-radius:10px;}
.top-round {-moz-border-radius-topright:10px;-moz-border-radius-topleft:10px;border-top-left-radius:10px;border-top-right-radius:10px;-webkit-border-top-left-radius:10px;-webkit-border-top-right-radius:10px;}
.bottom-left-round {-moz-border-radius-bottomleft:10px;border-bottom-left-radius:10px;-webkit-border-bottom-left-radius:10px;}
.bottom-right-round {-moz-border-radius-bottomright:10px;border-bottom-right-radius:10px;-webkit-border-bottom-right-radius:10px;}
.left-round {-moz-border-radius-bottomleft:10px;border-bottom-left-radius:10px;-webkit-border-bottom-left-radius:10px;-moz-border-radius-topleft:10px;border-top-left-radius:10px;-webkit-border-top-left-radius:10px;}
.round_except_top_left {-webkit-border-top-right-radius:10px;border-top-right-radius:10px;-moz-border-radius-topright:10px;-moz-border-radius-bottomright:10px;-moz-border-radius-bottomleft:10px;border-bottom-left-radius:10px;border-bottom-right-radius:10px;-webkit-border-bottom-left-radius:10px;-webkit-border-bottom-right-radius:10px;}
.round_except_bottom_left {-moz-border-radius-bottomright:10px;border-bottom-right-radius:10px;-webkit-border-bottom-right-radius:10px;-moz-border-radius-topright:10px;-moz-border-radius-topleft:10px;border-top-left-radius:10px;border-top-right-radius:10px;-webkit-border-top-left-radius:10px;-webkit-border-top-right-radius:10px;}

/*AJAX*/
.ajax_loading {height:400px;background:url("<%= asset_path 'ajax-loader.gif' %>") no-repeat center center;}

/*DROP SHADOWS*/
.box_shadow{
	-webkit-box-shadow: 0 2px 3px rgba(0,0,0,.2);
	-moz-box-shadow: 0 2px 3px rgba(0,0,0,.2);
	box-shadow: 0 2px 3px rgba(0,0,0,.2);
}

/*BUTTONS AND COLORS*/
.button {
	display: inline-block;
	zoom: 1; /* zoom and *display = ie7 hack for display:inline-block */
	*display: inline;
	vertical-align: baseline;
	margin: 0 2px;
	outline: none;
	cursor: pointer;
	text-align: center;
	text-decoration: none;
	font: 14px/100% Arial, Helvetica, sans-serif;
	padding: .5em 2em .55em;
	text-shadow: 0 1px 1px rgba(0,0,0,.3);
	-webkit-border-radius: .5em; 
	-moz-border-radius: .5em;
	border-radius: .5em;
	-webkit-box-shadow: 0 1px 2px rgba(0,0,0,.2);
	-moz-box-shadow: 0 1px 2px rgba(0,0,0,.2);
	box-shadow: 0 1px 2px rgba(0,0,0,.2);
}
.button:hover {
	text-decoration: none;
}
.button:active {
	position: relative;
	top: 1px;
}
 
.bigrounded {
	-webkit-border-radius: 2em;
	-moz-border-radius: 2em;
	border-radius: 2em;
}
.medium {
	font-size: 12px;
	padding: .4em 1.5em .42em;
}
.small {
	font-size: 11px;
	padding: .2em 1em .275em;
}
 
/* color styles 
---------------------------------------------- */
 
/* black */
.black {
	color: #d7d7d7;
	border: solid 1px #333;
	background: #333;
	background: -webkit-gradient(linear, left top, left bottom, from(#666), to(#000));
	background: -moz-linear-gradient(top,  #666,  #000);
	filter:  progid:DXImageTransform.Microsoft.gradient(startColorstr='#666666', endColorstr='#000000');
}
.black:hover {
	background: #000;
	background: -webkit-gradient(linear, left top, left bottom, from(#444), to(#000));
