/*RANKING*/
div#ranking_content h1#ranking_title {text-align:center;}
span.back_to_region_select {display:inline-block;width:60%;margin-left:20%;margin-right:20%;height:20px;cursor:pointer;color:#ffffff;line-height:20px;font-weight:bold;text-align:center;font-size:14px;margin-bottom:12px;}
span.back_to_region_select:hover {color:#bbbbbb;}
div.ranking_tabs {border:0px solid #ffffff;display:inline-block;margin-left:85px;width:440px;margin-right:85px;margin-bottom:24px;}
div.ranking_tabs ul.jqueryui-ranking-tabs {width:431px;background:#E6E6E6 url(images/ui-bg_glass_75_e6e6e6_1x400.png) 50% 50% repeat-x;display:inline-block;}
div.ranking_tabs ul.jqueryui-ranking-tabs li.ranking_tab {height:30px;line-height:30px;text-align:center;width:140px;display:inline-block;}
div.ranking_tabs ul.jqueryui-ranking-tabs li.ranking_tab a {text-align:center;display:inline-block;width:100%;}
div.score_explanation {color:#7a7a7a;position:absolute;width:240px;height:200px;background:#ffffff;border:1px solid #325d64;padding:12px;}
div.score_explanation h3 {font-size:16px;color:#0b3542;font-weight:bold;border-bottom:1px solid #0b3542;margin-bottom:3px;padding-bottom:3px;}
div.score_explanation p {text-align:justify;font-size:13px;}
div.star_explanation {color:#7a7a7a;position:absolute;width:200px;height:150px;background:#ffffff;border:1px solid #325d64;padding:12px;}
div.star_explanation h3 {font-size:16px;color:#0b3542;font-weight:bold;border-bottom:1px solid #0b3542;margin-bottom:3px;padding-bottom:3px;}
div.star_explanation ul {font-size:13px;list-style:none;padding:0px;margin:0px;float:left;display:inline-block;width:100%;margin-top:12px;}
div.star_explanation ul li {list-style-type:none;padding:0px;margin:0px;float:left;display:inline-block;width:100%;margin-bottom:12px;}
div.star_explanation ul li span.icon {display:inline-block;float:left;font-size:16px;text-align:left;width:20%;margin-right:5%;}
div.star_explanation ul li span.explanation {display:inline-block;float:right;text-align:left;width:65%;margin-left:0%;line-height:16px;}
.jqueryui-ranking-subselect {padding:0px;margin:0px;display:inline-block;width:431px;font-size:12px;border-left:1px solid #bcbcbc;border-right:1px solid #bcbcbc;}
.jqueryui-ranking-subselect li {padding:0px;margin:0px;list-style-type:none;display:inline-block;width:83px;text-align:center;height:30px;line-height:30px;cursor:pointer;padding-right:0px;}
.jqueryui-ranking-subselect .ui-selectee {color:#325d64; }
.jqueryui-ranking-subselect .ui-selecting { background: #FECA40; }
.jqueryui-ranking-subselect .ui-selected { background: #7895a3; color: white; height:30px;}
.jqueryui-ranking-subselect-diagnosis {padding:0px;margin:0px;display:inline-block;width:431px;font-size:12px;border-left:1px solid #bcbcbc;border-right:1px solid #bcbcbc;}
.jqueryui-ranking-subselect-diagnosis li {padding:0px;margin:0px;list-style-type:none;display:inline-block;width:140px;text-align:center;height:30px;line-height:30px;cursor:pointer;padding-right:0px;}
.jqueryui-ranking-subselect-diagnosis .ui-selectee {color:#325d64; }
.jqueryui-ranking-subselect-diagnosis .ui-selecting { background: #FECA40; }
.jqueryui-ranking-subselect-diagnosis .ui-selected { background: #7895a3; color: white; height:30px;}
div.clinic_rank_item {display:inline-block;width:100%;float:left;position:relative;border-bottom:1px solid #bcbcbc;}
div.clinic_rank_item span.clinic_contact_button {float:left;display:inline-block;width:90px;height:80px;text-align:center;line-height:26px;border:1px solid #cccccc;color:#ffffff;font-weight:bold;margin-top:34px;cursor:pointer;}
div.clinic_rank_item span.clinic_contact_button:hover {color:#cccccc;}
div.clinic_rank_item span.already_contacted {background:#f5f5f5;color:#7895a3;cursor:auto;float:left;display:inline-block;width:90px;height:80px;text-align:center;line-height:26px;border:0px solid #f5f5f5;font-weight:bold;margin-top:34px;}
div.clinic_rank_item span.register_to_contact {float:left;display:inline-block;width:90px;height:80px;text-align:center;line-height:26px;border:1px solid #cccccc;background:#7895a3;color:#ffffff;font-weight:bold;margin-top:34px;cursor:pointer;}
div.clinic_rank_item span.register_to_contact a {text-decoration:none;color:#ffffff;display:block;height:80px;}
div.clinic_rank_item span.register_to_contact a:hover {color:#cccccc;}
div.clinic_rank_item h3.clinic_name {display:inline-block;width:96%;margin-top:24px;margin-bottom:12px;margin-left:4%;}
ul.clinic_rank {margin:0px;padding:0px;display:inline-block;float:left;width:298px;margin:10px;padding-bottom:20px;margin-bottom:0px;}
ul.clinic_rank:last-child {border-bottom:0px;}
ul.clinic_rank.bottom {border-bottom:0px solid #ffffff;}
ul.clinic_rank li {list-style-type:none;display:inline-block;margin:0px;padding:0px;float:left;width:410px;overflow:hidden;}
ul.clinic_rank li.clinic_name {padding-left:0px;height:25px;line-height:25px;}
ul.clinic_rank li.clinic_name a {display:block;text-decoration:none;color:#325d5d;height:25px;line-height:25px;}
ul.clinic_rank li.clinic_name a:hover {color:#103b3b;}
ul.clinic_rank li.clinic_location {color:#666666;margin-bottom:10px;margin-left:20px;font-size:11px;}
ul.clinic_rank li.clinic_stats {padding-left:20px;width:60%;font-size:12px;padding-bottom:3px;padding-top:3px;}
ul.clinic_rank li.clinic_stats span.score_type {display:inline-block;float:left;height:16px;line-height:16px;}
ul.clinic_rank li.clinic_stats span.score_result {display:inline-block;float:right;}
ul.clinic_rank li.clinic_stats span.score_result span.icon {display:inline-block;padding-left:5px;font-size:16px;height:16px;}
span.all_state_label {font-size:14px;color:#325d64;}
div.ranking_tab_content {padding:0px;margin:0px;width:100%;display:inline-block;}
h2.age_ranking_title {padding:0px;margin:0px;width:431px;float:left;display:inline-block;text-align:center;border:1px solid #bcbcbc;border-top:0px solid #7895a3;color:#ffffff;height:30px;line-height:30px;border-bottom:0px solid #7895a3;}
h2.age_ranking_title.hidden {display:none;}
div.ranking_results {background:#f5f5f5;min-height:500px;display:inline-block;width:431px;border:1px solid #bcbcbc;border-top:0px solid #e5e5e5;}
div.ranking_results div.clinic_rank_list {display:inline-block;width:100%;min-height:500px;}
ul.ranking_nav {width:100%;padding:0px;margin:0px;height:30px;float:left;display:inline-block;}
ul.ranking_nav li {list-style-type:none;padding:0px;margin:0px;height:30px;display:inline-block;width:10%;color:#ffffff;font-size:20px;line-height:30px;padding-left:15px;padding-right:15px;cursor:pointer;}
ul.ranking_nav li span.icon {cursor:pointer;color:#0b3542;}
ul.ranking_nav li:hover {color:#cccccc;}
ul.ranking_nav li.left_button {float:left;text-align:left;}
ul.ranking_nav li.right_button {float:right;text-align:right;}

/*STATE SELECT*/
div.state_select{margin-top:84px;display:inline-block;width:266px;padding:12px;}
div.state_select.hidden {display:none;}
ul.state_select{list-style:none;padding:0px;margin:0px;width:100%;display:inline-block;float:left;}
ul.state_select li {list-style-type:none;padding:0px;margin:0px;display:inline-block;float:left;}
ul.state_select li.header {font-size:16px;font-family:'Lato', Arial, sans-serif;font-weight:300;color:#0b3542;padding-bottom:3px;border-bottom:1px solid #0b3542;margin-bottom:12px;}
ul.state_select li.all_state {width:100%;height:30px;line-height:30px;}
ul.state_select li.all_state.last{margin-bottom:12px;}
ul.state_select li.state {width:60px;font-size:12px;margin-left:6px;}
ul.state_select li.state input {float:right;margin-right:18px;display:inline-block;}


/*Hidden*/
.hidden {display:none;}
.inlineblock {display:inline-block;}