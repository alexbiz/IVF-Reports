              imprate_line_graph.Set('chart.ylabels', false);																					
              cycles_bar_graph.Set('chart.ymax', y_max);
              imprate_line_graph.Set('chart.tooltips', tooltips);


              cycles_bar_graph.Set('chart.line', imprate_line_graph);
              cycles_bar_graph.Draw();
            }
          });
        }
